{
  "name": "gist-auto-update",
  "version": "2.0.0",
  "description": "Atualizar o gist que tem as tasks de projetos do meu Todoist.",
  "main": "src/index.js",
  "scripts": {
    "build": "sucrase ./src -d ./build --transforms imports",
    "prestart": "npm run build",
    "start": "DEBUG=gist-auto-update* node build/",
    "dev": "DEBUG=gist-auto-update* nodemon --config nodemon.json",
    "clean": "rm -rf build/"
  },
  "author": "Micael Levi (micalevisk.github.io)",
  "license": "MIT",
  "engineStrict": true,
  "engines": {
    "node": ">=12.4.0"
  },
  "dependencies": {
    "@octokit/rest": "^16.35.2",
    "debug": "^4.1.1",
    "dotenv-safe": "^8.2.0",
    "node-fetch": "^2.2.0",
    "sucrase": "^3.10.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
